# specify the object files
OBJS = 	dcabs1.o \
	izamax.o \
	lsame.o \
	xerbla.o \
	zgemm.o \
	zgeru.o \
	zscal.o \
	zswap.o \
	ztrsm.o

#implicit rule to tell how to make object files from *.f (f77) files
%.o : %.f
	$(F77) -c $(FFLAGS_LIBS) $*.f

# make the library file
libmini_blas_s : $(OBJS)
	ar -r libmini_blas_s.a *.o;
	ranlib libmini_blas_s.a

# to clean up this library
.PHONY : clean realclean
clean :
	-rm $(OBJS) $(CLEANUP)
realclean : clean
	-rm *.a
